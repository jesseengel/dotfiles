[user]
    email = jesse.engel@gmail.com
    name = jesseengel
[push]
    default = simple
[core]
    editor = vim
[alias]
    su = status -uno
    br = branch
    co = checkout
    unstage = reset HEAD --
    rev = checkout --
    ci = commit
[filter "media"]
    required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "lfs"]
    clean = git lfs clean %f
    smudge = git lfs smudge %f
    required = true
